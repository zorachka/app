{
    "name": "zorachka/app",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Siarhei Bautrukevich",
            "email": "hello@bautrukevich.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*",
        "zorachka/framework": "dev-dev"
    },
    "autoload": {
        "psr-4": {
            "Project\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Project\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "pestphp/pest": "^1.21"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
